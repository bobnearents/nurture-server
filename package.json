{
  "name": "nurture-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node --experimental-json-modules index.js",
    "dev": "nodemon index.js",
    "db:backup": "./db/backup-database.sh",
    "db:restore": "./db/restore-recent-data.sh",
    "predb:rebuild": "npm run generate-sql",
    "db:rebuild": "npm run migrate 0 && npm run migrate && npm run seed",
    "db:update": "npm run db:backup && npm run rebuild",
    "generate-sql": "node ./db/model/generate-migrations.js && node ./db/model/generate-seeds.js",
    "seed-sheets": "./db/seed-sheets-data.sh",
    "migrate": "postgrator --config ./config/postgrator-config.js",
    "seed": "./db/seed-database.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/client-ses": "^3.445.0",
    "@google-analytics/data": "^3.2.1",
    "aws-sdk": "^2.1323.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fs": "^0.0.1-security",
    "googleapis": "^95.0.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^3.2.4",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "postgrator": "^7.1.0",
    "postgrator-cli": "^6.0.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1"
  },
  "type": "module",
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.10",
    "@types/multer-s3": "^3.0.3",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
