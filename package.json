{
  "name": "nurture-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "deploy": "git push heroku master && heroku logs --tail",
    "migrate": "postgrator --config ./config/postgrator-config.js",
    "seed": "psql -U postgres -d nurture -c \"BEGIN TRANSACTION;\" -f ./seeds/seed.certification.sql -f ./seeds/seed.payment.sql -f ./seeds/seed.service.sql -c \"COMMIT;\"",
    "refresh": "npm run migrate 0 && npm run migrate && npm run seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "googleapis": "^95.0.0",
    "node-fetch": "^3.2.4",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "postgrator": "^7.1.0"
  },
  "type": "module"
}
